<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.TextFlow?>

<?import javafx.scene.layout.HBox?>
<BorderPane prefHeight="700.0" prefWidth="1000.0" styleClass="grid_welcome" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.fhj.swengb.apps.battleship.jfx.BattleShipFxLicense">
   <center>


      <ScrollPane fx:id="scroll" maxHeight="500.0" maxWidth="600.0" styleClass="scroll" BorderPane.alignment="CENTER">
         <TextFlow fx:id="licenseText" maxHeight="450.0" maxWidth="550.0" styleClass="textflow" />
      </ScrollPane>

   </center>
   <bottom>

      <HBox prefHeight="27.0" prefWidth="250.0" styleClass="hbox" BorderPane.alignment="BOTTOM_LEFT">
         <Button fx:id="nextScreen" onAction="#back" prefHeight="35.0" prefWidth="170.0" styleClass="button" text="back" />
      </HBox>
   </bottom>
</BorderPane>
